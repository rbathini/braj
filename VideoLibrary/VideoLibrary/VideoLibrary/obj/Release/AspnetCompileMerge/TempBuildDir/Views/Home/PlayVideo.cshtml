@{

    ViewBag.Title = "PlayVideo";

    var videoKey = Request.QueryString["Video"];
    var url = "http://www.youtube.com/v/" + videoKey;
}
<h2>
    PlayVideo</h2>
<div id="divVideoPlayer">
    <object
data="@url">
    </object>
</div>
